num = 10
print(2*num*'-','原始center')
for row in range(num):
    print('{:<8d}'.format(row + 1), end='')
    print(((row+1) * '*').center(20, ' '))
 
print('\n', 2*num*'-', '改进版本1-奇数-center')
for row in range(1, 2*num, 2):
    print('{:<8d}'.format(row), end='')
    print((row * '*').center(20, ' '))
